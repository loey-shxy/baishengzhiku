view {
  box-sizing: border-box;
}
body,
uni-app,
uni-page,
uni-page-wrapper,
uni-page-body {
  height: 100%;
}
.app-container {
  height: 100%;
  color: #000;
}
.app-content {
  height: calc(100% - 118px);
}
.app-header {
  padding-top: 118px;
  position: -webkit-sticky;
  position: sticky;
  background-color: #fff;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
}
.app-header .app-header-content {
  height: 44px;
  line-height: 44px;
  position: absolute;
  left: 0;
  bottom: 0;
  width: 100%;
  text-align: center;
  font-size: 17px;
  font-weight: bold;
}
.app-header .app-header-content .arrow-left {
  position: absolute;
  left: 12px;
}
.search-box {
  position: relative;
  margin: 18px 17px 17px;
  height: 33px;
}
.search-box .search-icon {
  width: 16px;
  height: 16px;
  margin-left: 10px;
}
.search-box .uni-easyinput {
  height: 33px;
}
.search-box .uni-easyinput .uni-easyinput__content {
  background-color: #F9F9F9 !important;
  border-radius: 17px;
}
.search-box .uni-easyinput .uni-easyinput__content-input {
  height: 33px;
}
.flex {
  display: flex;
}
.align-items-center {
  align-items: center;
}
.align-items-start {
  align-items: flex-start;
}
.justify-center {
  justify-content: center;
}
.justify-between {
  justify-content: space-between;
}
.justify-around {
  justify-content: space-around;
}
.news-status {
  position: absolute;
  left: 21px;
  bottom: 23px;
  border-radius: 4px;
  padding: 0 8px;
  line-height: 22px;
  color: #fff;
  z-index: 3;
  font-size: 12px;
}
.news-status.type_free {
  background-color: #EFC37F;
  color: #000;
}
.news-status.type_not-subscribed {
  background-color: #C53B2E;
}
.news-status.type_subscribed {
  background-color: #0752A5;
}
.news-item {
  margin-top: 10px;
  background-color: #fff;
  padding: 15px 17px 18px;
  display: flex;
  position: relative;
}
.news-item_image {
  width: 120px;
  height: 91px;
  border-radius: 3px;
  flex-shrink: 0;
}
.news-item_image.size-medium {
  width: 120px;
  height: 91px;
}
.news-item_image.size-large {
  width: 140px;
  height: 110px;
}
.news-item_info {
  padding: 11px 0 8px 18px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.news-item_info .news-title {
  font-weight: bold;
}
.news-item_info .news-content {
  color: #666;
  font-size: 12px;
}
.news-item_info .news-footer {
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #9E9E9E;
  font-size: 12px;
}
.services-news-item {
  margin-top: 8px;
  background-color: #fff;
  padding: 16px;
  position: relative;
}
.services-news-item .news-status {
  bottom: 80px;
}
.services-news-item_footer {
  margin-top: 10px;
  height: 32px;
}
.services-news-item_footer .news-footer-price {
  color: #666;
  font-size: 12px;
}
.services-news-item_footer .news-footer-price .price-unit {
  color: #C53B2E;
  font-size: 12px;
  margin-left: 4px;
}
.services-news-item_footer .news-footer-price .price {
  color: #C53B2E;
  font-size: 17px;
  font-weight: bold;
}
.services-news-item_footer .subscribe-btn {
  height: 32px;
  line-height: 32px;
  background: linear-gradient(90deg, #0752A5 0%, #2575CD 100%);
  border-radius: 6px;
  color: #fff;
  width: 100px;
  margin: unset;
}
.services-news-title {
  margin-top: 12px;
  font-weight: bold;
}
.services-news-subtitle {
  margin-top: 7px;
  color: #666;
  font-size: 12px;
}
.uni-easyinput {
  width: 100%;
  flex: 1;
  position: relative;
  text-align: left;
  color: #333;
  font-size: 14px;
}
.uni-easyinput__content {
  flex: 1;
  width: 100%;
  display: flex;
  box-sizing: border-box;
  flex-direction: row;
  align-items: center;
  border-color: #fff;
  transition-property: border-color;
  transition-duration: 0.3s;
}
.uni-easyinput__content-input {
  width: auto;
  position: relative;
  overflow: hidden;
  flex: 1;
  line-height: 1;
  font-size: 14px;
  height: 35px;
  /*ifdef H5*/
  /*endif*/
}
.uni-easyinput__content-input ::-ms-reveal {
  display: none;
}
.uni-easyinput__content-input ::-ms-clear {
  display: none;
}
.uni-easyinput__content-input ::-o-clear {
  display: none;
}
.uni-easyinput__placeholder-class {
  color: #999;
  font-size: 12px;
}
.is-textarea {
  align-items: flex-start;
}
.is-textarea-icon {
  margin-top: 5px;
}
.uni-easyinput__content-textarea {
  position: relative;
  overflow: hidden;
  flex: 1;
  line-height: 1.5;
  font-size: 14px;
  margin: 6px;
  margin-left: 0;
  height: 80px;
  min-height: 80px;
  min-height: 80px;
  width: auto;
}
.input-padding {
  padding-left: 10px;
}
.content-clear-icon {
  padding: 0 5px;
}
.label-icon {
  margin-right: 5px;
  margin-top: -1px;
}
.is-input-border {
  display: flex;
  box-sizing: border-box;
  flex-direction: row;
  align-items: center;
  border: 1px solid #F0F0F0;
  border-radius: 4px;
}
.uni-error-message {
  position: absolute;
  bottom: -17px;
  left: 0;
  line-height: 12px;
  color: #e43d33;
  font-size: 12px;
  text-align: left;
}
.uni-error-msg--boeder {
  position: relative;
  bottom: 0;
  line-height: 22px;
}
.is-input-error-border {
  border-color: #e43d33;
}
.is-input-error-border .uni-easyinput__placeholder-class {
  color: #f29e99;
}
.uni-easyinput--border {
  margin-bottom: 0;
  padding: 10px 15px;
  border-top: 1px #eee solid;
}
.uni-easyinput-error {
  padding-bottom: 0;
}
.is-first-border {
  border: none;
}
.is-disabled {
  background-color: #f7f6f6;
  color: #d5d5d5;
}
.is-disabled .uni-easyinput__placeholder-class {
  color: #d5d5d5;
  font-size: 12px;
}